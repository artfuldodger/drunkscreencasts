<div class="jumbotron">
  <div class="container">
    <h1><%= @video.title %></h1>
    <p>Published on <%= l @video.published_at.to_date %></p>
    <p><%= @video.description %></p>
  </div>
</div>

<iframe type="text/html" width="1280" height="780"
    src="http://www.youtube.com/embed/<%= @video.youtube_id %>"
    frameborder="0"></iframe>
